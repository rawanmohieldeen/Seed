<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="snippet_options_inherited" inherit_id="website.snippet_options">
		
		
		<!-- Footers -->
		<xpath expr="//we-select[@data-variable='footer-template']" position="inside">

            <we-button title="Footer Three"
                       data-customize-website-views="nm_health_wellness_website.theme_common_footer3"
                       data-customize-website-variable="'footer_three'"
                       data-img="/nm_health_wellness_website/static/src/img/footer_three.png"/>
		</xpath>

	</template>

	<template id="theme_common_footer3" inherit_id="website.layout" name="Footer Three" active="False">
<div id="footer" position ="replace">

    <div id="footer" class="oe_structure oe_structure_solo" style="" data-oe-id="1605" data-oe-xpath="/data/xpath/div" data-oe-model="ir.ui.view" data-oe-field="arch">
      <section class="s_text_block pt32 pb16" data-snippet="s_text_block" data-name="Text" style="background-image: none;" data-original-title="" title="" aria-describedby="tooltip894014">
        <div class="container">
          <div class="row align-items-end">
            <div class="o_colored_level pt72 pb48 col-lg-3" style="">
              <p class="mb-2"><u>Call us</u><br/>+974-XXXX-XXXX<br/><u>Email us</u><br/>hello@seed.qa<br/><u>Follow us</u><br/></p>
            </div>
            <div class="pb16 o_colored_level pt24 col-lg-3" data-original-title="" title="" aria-describedby="tooltip670601">
